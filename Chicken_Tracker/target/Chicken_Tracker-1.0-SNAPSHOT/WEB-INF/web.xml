<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <listener>
        <listener-class>br.com.avicultura.chicken_tracker.ServletListener.AppContextListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>executarLogin</servlet-name>
        <servlet-class>br.com.avicultura.chicken_tracker.Servlets.Login.executarLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PerfilServlet</servlet-name>
        <servlet-class>br.com.avicultura.chicken_tracker.Servlets.Perfil.PerfilServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProdutoServlet</servlet-name>
        <servlet-class>br.com.avicultura.chicken_tracker.Servlets.Produto.ProdutoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VacinaServlet</servlet-name>
        <servlet-class>br.com.avicultura.chicken_tracker.Servlets.Vacina.VacinaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NegocioServlet</servlet-name>
        <servlet-class>br.com.avicultura.chicken_tracker.Servlets.Negocio.NegocioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EstabelecimentoServlet</servlet-name>
        <servlet-class>br.com.avicultura.chicken_tracker.Servlets.Estabelecimentos.EstabelecimentoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LocalAvesServlet</servlet-name>
        <servlet-class>br.com.avicultura.chicken_tracker.Servlets.LocalAves.LocalAvesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FornecimentoServlet</servlet-name>
        <servlet-class>br.com.avicultura.chicken_tracker.Servlets.Fornecimento.FornecimentoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FornecedorServlet</servlet-name>
        <servlet-class>br.com.avicultura.chicken_tracker.Servlets.Fornecedor.FornecedorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FuncionarioServlet</servlet-name>
        <servlet-class>br.com.avicultura.chicken_tracker.Servlets.Funcionario.FuncionarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PagamentoServlet</servlet-name>
        <servlet-class>br.com.avicultura.chicken_tracker.Servlets.Pagamento.PagamentoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmailServlet</servlet-name>
        <servlet-class>br.com.avicultura.chicken_tracker.Servlet.Email.EmailServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EncherBanco</servlet-name>
        <servlet-class>br.com.avicultura.chicken_tracker.Utils.EncherBanco</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>executarLogin</servlet-name>
        <url-pattern>/executarLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PerfilServlet</servlet-name>
        <url-pattern>/PerfilServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProdutoServlet</servlet-name>
        <url-pattern>/ProdutoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VacinaServlet</servlet-name>
        <url-pattern>/VacinaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NegocioServlet</servlet-name>
        <url-pattern>/NegocioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EstabelecimentoServlet</servlet-name>
        <url-pattern>/EstabelecimentoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LocalAvesServlet</servlet-name>
        <url-pattern>/LocalAvesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FornecimentoServlet</servlet-name>
        <url-pattern>/FornecimentoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FornecedorServlet</servlet-name>
        <url-pattern>/FornecedorServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FuncionarioServlet</servlet-name>
        <url-pattern>/FuncionarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PagamentoServlet</servlet-name>
        <url-pattern>/PagamentoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmailServlet</servlet-name>
        <url-pattern>/EmailServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EncherBanco</servlet-name>
        <url-pattern>/EncherBanco</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <error-page>
        <error-code>404</error-code>
        <location>/excecoes/erro404.jsp</location>
    </error-page>
    <error-page>
        <exception-type>java.lang.*</exception-type>
        <location>/excecoes/ErroBanco.jsp</location>
    </error-page>
    <error-page>
        <location>/excecoes/erro404.jsp</location>
    </error-page>
</web-app>
